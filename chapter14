        // Generate question functions
        function generateQuestion141() {
            const randomIndex = Math.floor(Math.random() * introTwoWayAnovaQuestions.length);
            currentQuestion141 = introTwoWayAnovaQuestions[randomIndex];
            displayQuestion(141, currentQuestion141);
        }
        
        function generateQuestion142() {
            const randomIndex = Math.floor(Math.random() * mainEffectsInteractionQuestions.length);
            currentQuestion142 = mainEffectsInteractionQuestions[randomIndex];
            displayQuestion(142, currentQuestion142);
        }
        
        function generateQuestion143() {
            const randomIndex = Math.floor(Math.random() * sumSquaresFTestQuestions.length);
            currentQuestion143 = sumSquaresFTestQuestions[randomIndex];
            displayQuestion(143, currentQuestion143);
        }
        
        function generateQuestion144() {
            const randomIndex = Math.floor(Math.random() * simpleEffectsMultipleQuestions.length);
            currentQuestion144 = simpleEffectsMultipleQuestions[randomIndex];
            displayQuestion(144, currentQuestion144);
        }
        
        function generateQuestion145() {
            const randomIndex = Math.floor(Math.random() * effectSizesPowerQuestions.length);
            currentQuestion145 = effectSizesPowerQuestions[randomIndex];
            displayQuestion(145, currentQuestion145);
        }
        
        function generateQuestion146() {
            const randomIndex = Math.floor(Math.random() * assumptionsExtensionsQuestions.length);
            currentQuestion146 = assumptionsExtensionsQuestions[randomIndex];
            displayQuestion(146, currentQuestion146);
        }

        // Helper function to display questions
        function displayQuestion(sectionNum, question) {
            document.getElementById(`question-${sectionNum}-content`).innerHTML = question.question;
            document.getElementById(`hint-${sectionNum}`).innerHTML = `üí° ${question.hint}`;
            
            document.getElementById(`answer-${sectionNum}`).value = '';
            document.getElementById(`feedback-${sectionNum}`).style.display = 'none';
            document.getElementById(`steps-${sectionNum}`).style.display = 'none';
        }

        // Check answer functions
        function checkAnswer141() { checkAnswer(141, currentQuestion141); }
        function checkAnswer142() { checkAnswer(142, currentQuestion142); }
        function checkAnswer143() { checkAnswer(143, currentQuestion143); }
        function checkAnswer144() { checkAnswer(144, currentQuestion144); }
        function checkAnswer145() { checkAnswer(145, currentQuestion145); }
        function checkAnswer146() { checkAnswer(146, currentQuestion146); }

        // Show steps functions  
        function showSteps141() { showSteps(141, currentQuestion141); }
        function showSteps142() { showSteps(142, currentQuestion142); }
        function showSteps143() { showSteps(143, currentQuestion143); }
        function showSteps144() { showSteps(144, currentQuestion144); }
        function showSteps145() { showSteps(145, currentQuestion145); }
        function showSteps146() { showSteps(146, currentQuestion146); }

        // Helper functions
        function checkAnswer(sectionNum, currentQuestion) {
            const userAnswer = document.getElementById(`answer-${sectionNum}`).value.toLowerCase().trim();
            if (!currentQuestion) return;
            
            const isCorrect = currentQuestion.correctAnswers.some(answer => 
                userAnswer.includes(answer.toLowerCase()) || answer.toLowerCase().includes(userAnswer)
            );
            
            const feedbackElement = document.getElementById(`feedback-${sectionNum}`);
            if (isCorrect) {
                feedbackElement.innerHTML = `<div class="correct">‚úÖ Correct! ${currentQuestion.explanation}</div>`;
                feedbackElement.className = 'question-feedback correct-feedback';
            } else {
                feedbackElement.innerHTML = `<div class="incorrect">‚ùå Incorrect. ${currentQuestion.explanation}</div>`;
                feedbackElement.className = 'question-feedback incorrect-feedback';
            }
            feedbackElement.style.display = 'block';
        }

        function showSteps(sectionNum, currentQuestion) {
            if (!currentQuestion) return;
            const stepsElement = document.getElementById(`steps-${sectionNum}`);
            stepsElement.innerHTML = currentQuestion.steps;
            stepsElement.style.display = 'block';
        }

        // Initialize on page load
        window.onload = function() {
            generateQuestion141();
            generateQuestion142(); 
            generateQuestion143();
            generateQuestion144();
            generateQuestion145();
            generateQuestion146();
        }
    </script>
</body>
</html>
